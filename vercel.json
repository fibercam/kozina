{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": [
          "requirements.txt"
        ]
      }
    }
  ],
  "routes": [
    { "src": "/remove-bg", "methods": ["POST"], "dest": "app.py" },
    { "src": "/(.*)", "dest": "app.py" }
  ]
}
